		Если Номенклатура = Неопределено Тогда
			СтруктураДанных.Вставить("Статус", 404);
			СтруктураДанных.Вставить("ОписаниеОшибки", "Объект Номенклатура не был найден " + Строка(стр.Номенклатура));
